{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "desc": {
      "type": "text"
    },
    "img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "img2": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "date": {
      "type": "date"
    },
    "time": {
      "type": "time"
    },
    "brands": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::brand.brand",
      "inversedBy": "courses"
    },
    "sub_course_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::sub-course-category.sub-course-category",
      "inversedBy": "courses"
    },
    "isPopular": {
      "type": "boolean",
      "default": false
    },
    "isNew": {
      "type": "boolean",
      "default": true
    },
    "area": {
      "type": "string"
    },
    "goal": {
      "type": "string"
    },
    "tags": {
      "type": "text"
    },
    "searches": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::search.search",
      "inversedBy": "courses"
    }
  }
